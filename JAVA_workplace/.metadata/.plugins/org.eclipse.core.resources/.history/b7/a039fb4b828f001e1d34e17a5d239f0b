package TestPackage;
class operators
{
	void unaryIncreamentmethod()
	{
		int a=30;
		System.out.println("Prefix value of "+ ++a);
		System.out.println("Postfix value of "+ a++);
	}
	void unarydecreamentfixmethod()
	{
		int b=30;
		System.out.println("Postfixoperator-"+ b++);
	}
}

