package TestPackage;

public class Loopstatementeg
{
	void EvenOdd() 
	{
		System.out.println("Even numbers are:");
		
		int i=1;
		while(i<=10)
		{
			if(i%2==0)
				System.out.println(i);			
		}
		i++;
}
	{System.out.println("Odd numbers are");
	int j=1;
	{
	while(j<=5)
	{
		if(j%2!=0)
			System.out.println(j);			
	}
	j++;}}
}
 class LoopStatement {

	public static void main(String[] args) {
		
		Loopstatementeg eo =new Loopstatementeg();
		eo.EvenOdd();
	}}