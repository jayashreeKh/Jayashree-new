package TestPackage;
class Loopstatementeg
{
	void EvenOdd() {
		System.out.println("Even numbers are:");
		
		int i=1;
		while(i<=10)
		{
			if(i%2==0)
				System.out.println(i);			
		}
		i++;
		}
	{
	int j=1;
	{
	while(j<=10)
	{
		if(j%2!=0)
			System.out.println(j);			
	}
	j++;}}
}
public class LoopStatement {

	public static void main(String[] args) {
		Loopstatementeg EO=new Loopstatementeg();
		EO.EvenOdd();
	}
}
