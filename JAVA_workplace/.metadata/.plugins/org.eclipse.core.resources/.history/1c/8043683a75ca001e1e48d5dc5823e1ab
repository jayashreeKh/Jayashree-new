package TestPackage;

class W
{
	void reverse()
	{
		int i=20;
		System.out.println("Reverse order for :");
		while(i>=10)
		{
			System.out.println(i);
		}i--;
	}
}

public class WhileLoopReverse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

W r=new W();
r.reverse();
	}}